<h1> Create your Instagaram Entry! </h1>

<%= form_for(@listing) do |f| %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :country %>
    <%= f.text_field :country %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    

 <h2>Upload your Photo to the World:</h2>

	
	<%= f.label :images %>
	<%= f.file_field :images, multiple: true%>


	<%= f.submit %>
	


<% end %>