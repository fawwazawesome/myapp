<h1>Edit Listing</h1>

<%= form_for(@listing) do |f| %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :country %>
    <%= f.text_field :country %>

    <%= f.label :description %>
    <%= f.text_field :description %>

	<%= f.label :images %>
	<%= f.file_field :images, multiple: true %>

	<%= f.submit %>
<% end %>



